<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddAccount</servlet-name>
        <servlet-class>AddAccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAccount</servlet-name>
        <url-pattern>/addaccount</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PostFinancialStatus</servlet-name>
        <servlet-class>PostFinancialStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PostFinancialStatus</servlet-name>
        <url-pattern>/financialstatus</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TransferMoney</servlet-name>
        <servlet-class>TransferMoney</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransferMoney</servlet-name>
        <url-pattern>/transfer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PostTransfers</servlet-name>
        <servlet-class>PostTransfers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PostTransfers</servlet-name>
        <url-pattern>/posttransfers</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>